{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check:eslint": "eslint --ext .jsx,.js,.tsx,.ts ./src",
    "check:prettier": "prettier --check ./src",
    "check": "run-p --continue-on-error check:*",
